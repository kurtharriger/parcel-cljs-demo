    "build": "yarn release && yarn html && yarn serve"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "http-server": "^0.12.3",
    "shadow-cljs": "^2.10.12"
  },
  "dependencies": {
    "parcel": "^2.0.1",
    "parcel-plugin-externals": "^0.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sakura.css": "^1.3.1"
  },
  "externals": [
    "cljs-main",
    "cljs-lib"
  ],
  "alias": {
    "cljs-main": "./target/main.js",
    "cljs-lib": "./target/lib.js"
  }
}
